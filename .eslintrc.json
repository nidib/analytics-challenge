{
	"env": {
		"browser": true,
		"es2020": true,
		"jest/globals": true
	},
	"extends": [
		"plugin:react/recommended",
		"airbnb"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 11,
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"comma-dangle": "off",
		"arrow-parens": "off",
		"eol-last": "off",
		"no-tabs": "off",
		"indent": ["error", "tab"],
		"import/prefer-default-export": "off",
		"import/no-unresolved": "off",
		"react/jsx-props-no-spreading": "off"
	}
}
